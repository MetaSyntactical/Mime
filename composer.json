{
    "name": "metasyntactical/mime",
    "type": "library",
    "description": "The MetaSyntactical MIMEtype detection component",
    "keywords": ["library", "mime"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Kreuer",
            "email": "d.kreuer@danielkreuer.com"
        }
    ],
    "require": {
        "php": "8.0.*",
        "metasyntactical/io": "^2.0"
    },
    "require-dev": {
        "infection/infection": "^0.24.0",
        "phpunit/phpunit": "^9.5",
        "mikey179/vfsstream": "^v1.6.9",
        "vimeo/psalm": "^4.8.1"
    },
    "autoload": {
        "psr-4": {
            "MetaSyntactical\\Mime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MetaSyntactical\\Mime\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        }
    }
}
